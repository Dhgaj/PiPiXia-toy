# 测试全局变量动态初始化

# 全局变量 - 使用复杂表达式初始化
let g_complex: int = 2 + 3 * 4  # 应该是 14
let g_nested: int = (5 + 3) * 2  # 应该是 16
let g_float: double = 1.5 + 2.5 * 3.0  # 应该是 9.0

# 常量全局变量（应该正常工作）
let g_const: int = 42
let g_const_str: string = "Hello World"

func main(): int {
    print("===== 全局变量动态初始化测试 =====")
    print("")
    
    print("测试1: 复杂表达式初始化")
    print("  g_complex = 2 + 3 * 4 = ", nowrap)
    print(g_complex)
    print("  (应输出: 14)")
    print("")
    
    print("测试2: 嵌套表达式初始化")
    print("  g_nested = (5 + 3) * 2 = ", nowrap)
    print(g_nested)
    print("  (应输出: 16)")
    print("")
    
    print("测试3: 浮点数表达式初始化")
    print("  g_float = 1.5 + 2.5 * 3.0 = ", nowrap)
    print(g_float)
    print("  (应输出: 9)")
    print("")
    
    print("测试4: 常量初始化（对比）")
    print("  g_const = ", nowrap)
    print(g_const)
    print("  g_const_str = ", nowrap)
    print(g_const_str)
    print("")
    
    print("===== 测试完成 =====")

    return 0
}
