# 减法运算测试
# 测试整数、浮点数的减法运算

func main(): int {
    print("========== 减法运算测试 ==========")
    print("")
    
    # 整数常量减法
    print("1. 整数常量减法:")
    let r1: int = 20 - 10
    let r2: int = 10 - 20
    let r3: int = -10 - 20
    let r4: int = 50 - 20 - 10
    print("  20 - 10 = ${r1} (应输出: 10)")
    print("  10 - 20 = ${r2} (应输出: -10)")
    print("  -10 - 20 = ${r3} (应输出: -30)")
    print("  50 - 20 - 10 = ${r4} (应输出: 20)")
    print("")
    
    # 整数变量减法
    let x: int = 25
    let y: int = 15
    let z: int = -10
    
    print("2. 整数变量减法:")
    print("  x = ${x}, y = ${y}, z = ${z}")
    let xy: int = x - y
    let yx: int = y - x
    let xz: int = x - z
    let xyz: int = x - y - z
    print("  x - y = ${xy} (应输出: 10)")
    print("  y - x = ${yx} (应输出: -10)")
    print("  x - z = ${xz} (应输出: 35)")
    print("  x - y - z = ${xyz} (应输出: 20)")
    print("")
    
    # 浮点数减法
    let a: double = 20.5
    let b: double = 10.3
    let c: double = -5.2
    
    print("3. 浮点数减法:")
    print("  a = ${a}, b = ${b}, c = ${c}")
    let ab: double = a - b
    let ba: double = b - a
    let ac: double = a - c
    let abc: double = a - b - c
    print("  a - b = ${ab} (应输出: 10.2)")
    print("  b - a = ${ba} (应输出: -10.2)")
    print("  a - c = ${ac} (应输出: 25.7)")
    print("  a - b - c = ${abc} (应输出: 15.4)")
    print("")
    
    # 混合类型减法
    print("4. 整数和浮点数混合运算:")
    let xa: double = x - a
    let ay: double = a - y
    let zc: double = z - c
    print("  x - a = ${xa} (应输出: 4.5)")
    print("  a - y = ${ay} (应输出: 5.5)")
    print("  z - c = ${zc} (应输出: -4.8)")
    print("")
    
    # 复杂表达式
    print("5. 复杂表达式:")
    let complex1: double = (x - y) - (a - b)
    let complex2: double = x - 5 - a + 2.5
    print("  (x - y) - (a - b) = ${complex1} (应输出: -0.2)")
    print("  x - 5 - a + 2.5 = ${complex2} (应输出: 2.0)")
    print("")
    
    # 边界值测试
    print("6. 边界值测试:")
    let zero: int = 0
    let xz_result: int = x - zero
    let self_sub: int = x - x
    print("  x - 0 = ${xz_result} (应输出: 25)")
    print("  x - x = ${self_sub} (应输出: 0)")
    print("")
    
    print("减法测试完成！")
    return 0
}
